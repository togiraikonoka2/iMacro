VERSION BUILD=9030808 RECORDER=FX
SET !EXTRACT_TEST_POPUP NO
TAG POS=1 TYPE=A ATTR=ID:npanchor EXTRACT=TXT

SET !VAR1 {{!Extract}}
SET !VAR2 EVAL("var np = \"{{!VAR1}}\";var nps = np.replace(/,/g, ''); npa = nps - 1200; npa")

SET !TIMEOUT_STEP 1
TAG POS=1 TYPE=INPUT:TEXT FORM=ACTION:process_bank.phtml ATTR=NAME:amount CONTENT={{!VAR2}}
WAIT SECONDS=2
ONDIALOG POS=1 BUTTON=OK CONTENT=
SET !TIMEOUT_STEP 1
TAG POS=1 TYPE=INPUT:SUBMIT FORM=ACTION:process_bank.phtml ATTR=*